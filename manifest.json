{
  "name": "Arxiv downloader with title",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "arxiv pdf renamer",
  "icons": {
    "16": "icon1616.png",
    "48": "icon4848.png",
    "128": "icon128128.png"
},
  "content_scripts": [{
    "matches": ["https://arxiv.org/abs/*"],
    "js": [
      "content.js"
    ]
  }],
  "background": {
		"scripts": ["bg.js"]
  },
  "browser_action": {
	}
  }